</head>    

<div id="Mensaje" class="row" style="padding-top: 80px;">
         <div class="col-md-2 offset-md-1">
<form action="/contacto" method="post">
            <p></p>
            <input type="text" name="nombre" class="form.control" id="input_name" placeholder="Nombre Completo">
            <br>
            <br>
            <input type="email" name="email" class="form.control" id="input_email" placeholder="Email">
            <br>
            <br>
            <input type="text" name="tema" class="form.control" id="input_tema" placeholder="Tema">
            <br>
            <br>

            <textarea name="mensaje" id="Mensaje" cols="30" rows="10" placeholder="Mensaje"></textarea>

            <button type="enviar" class="btn btn-primary">Enviar</button>
            </form>
            <!-- FIN MENSAJE -->
            <br>
        {{message}}

        </div>

        <!-- UBICACION -->
        <div class=" col-2 md-8 offset-md-1 ">
            <p>
                <img src="images/Ubicacion.jpg" alt="ubicacion" id="ubicacion">
                Ubicación
            <p style="color: black; background-color: rgb(255, 166, 0);"> Coronel Beltrán 170
                <br>
                Remedios De Escalada,Lanus
            </p>

            </p>

            <!-- MAPA-->
            <div id="Mapa" class="embebed-responsive-item">
